
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Profile</title>
</head>
<body>
    <div>
        <a class="btn btn-danger" href="/student/home">Home</a>
        <a class="btn btn-danger" href="/student/profile">Profile</a>
        <a class="btn btn-danger" href="/student/education">Education</a>
        <a class="btn btn-danger" href="/student/projects">Projects</a>
        <a class="btn btn-danger" href="/student/references">References</a>

        <h2>Profile</h2>

        <table>
            <tbody>
                <tr>
                    <td>Name</td>
                    <td>:</td>
                    <td>@ViewBag.Name</td>
                </tr>
                <tr>
                    <td>Email</td>
                    <td>:</td>
                    <td>@ViewBag.Email</td>
                </tr>
                <tr>
                    <td>Phone</td>
                    <td>:</td>
                    <td>@ViewBag.Phone</td>
                </tr>
            </tbody>
        </table>

        @*@ViewData["Goods"]*@
    </div>
</body>
</html>
